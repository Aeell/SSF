{
  "name": "super-slam-football",
  "version": "1.0.0",
  "type": "module",
  "description": "A multiplayer football game with special abilities",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node scripts/start-server.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node scripts/start-server.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "clean": "rimraf dist/ && node scripts/clean.js",
    "analyze": "webpack-bundle-analyzer stats.json",
    "check": "npm run lint && npm run format && npm run clean"
  },
  "keywords": [
    "game",
    "football",
    "multiplayer",
    "websocket",
    "threejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cannon-es": "^0.20.0",
    "colyseus": "^0.15.57",
    "colyseus.js": "^0.15.27",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "socket.io": "^4.8.1",
    "three": "^0.157.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@colyseus/monitor": "^0.15.8",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "vite": "^5.4.14",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
